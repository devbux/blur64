{
  "printWidth": 120,
  "proseWrap": "never",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "requirePragma": false,
  "insertPragma": false,
  "singleAttributePerLine": true,
  "endOfLine": "lf",
  "overrides": []
}
